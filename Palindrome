import java.util.*;

class Palindrom{
	
	public static boolean isPalindrome(int num) {
	  int sum=0,temp;
		temp=num;
		
		while(num>0) {
			
			sum=sum*10+(num%10);
			num=num/10;
		}
		return sum==temp;
			
	}
	
	public static void main(String[] args){
     Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
	 if(isPalindrome(n))
		 System.out.println(n+" is palindrome");
	 else
		 System.out.println(n+" is not a palindrome");
  
}
}
