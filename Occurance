import java.util.*;
 class occrance {
	public static void main(String[] args) {
				int count=0;
				Scanner sc=new Scanner(System.in);
				String sentence=sc.nextLine();
				String Word=sc.next();
				String[] a=sentence.split(" ");
				for(int i=0;i<a.length;i++)
					if(a[i].equals(Word))
							count++;
					System.out.println(Word+":"+count);
					
			}

		}

